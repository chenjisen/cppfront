main: () = {
    std::cout << "test parser: " << (1+2*3-4/5|6&7) << "\n";
    numbers: std::vector = ( -1, 123 );
    std::cout << numbers[0] << " -> " << foo( numbers[0] ) << "\n";
    std::cout << numbers[1] << " -> " << foo( numbers[1] ) << "\n";
}

foo: (n: int) -> double = {
    return n / 2.0;
}